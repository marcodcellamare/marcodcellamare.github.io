{
	"compilerOptions": {
		"baseUrl": ".",
		"composite": true,
		"paths": {
			"!package": [
				"package.json"
			],
			"!config": [
				"src/config.json"
			],
			"!/app/*": [
				"src/app/*"
			],
			"!/assets/*": [
				"src/assets/*"
			],
			"!/hooks/*": [
				"src/hooks/*"
			],
			"!/contexts/*": [
				"src/contexts/*"
			],
			"!/styles/*": [
				"src/styles/*"
			],
			"!/types/*": [
				"src/types/*"
			],
			"!/utils/*": [
				"src/utils/*"
			]
		}
	}
}