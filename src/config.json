{
	"domain": "https://marcodcellamare.github.io",
	"locale": {
		"default": "en-GB",
		"defaultNS": "default",
		"allowed": [
			"en-GB"
		],
		"ns": {
			"_": [
				"info"
			],
			"%ISO%": [
				"default",
				"com",
				"nav",
				"page.Home",
				"page.Resume",
				"page.Projects"
			]
		}
	},
	"fonts": [
		{
			"family": "Open Sans",
			"weights": [
				400,
				700
			],
			"italic": false
		},
		{
			"family": "Montserrat",
			"weights": [
				500,
				800
			],
			"italic": false
		}
	],
	"preload": [
		{
			"rel": "dns-prefetch",
			"href": "https://fonts.googleapis.com"
		},
		{
			"rel": "dns-prefetch",
			"href": "https://fonts.gstatic.com"
		},
		{
			"rel": "dns-prefetch",
			"href": "https://www.googletagmanager.com"
		},
		{
			"rel": "dns-prefetch",
			"href": "https://www.google-analytics.com"
		},
		{
			"rel": "preconnect",
			"href": "https://fonts.googleapis.com"
		},
		{
			"rel": "preconnect",
			"href": "https://fonts.googleapis.com",
			"crossorigin": "anonymous"
		},
		{
			"rel": "preconnect",
			"href": "https://www.googletagmanager.com"
		},
		{
			"rel": "preconnect",
			"href": "https://www.google-analytics.com"
		},
		{
			"rel": "preconnect",
			"href": "https://www.googletagmanager.com/gtag/js?id=%REACT_APP_ANALYTICS%",
			"as": "script"
		},
		{
			"rel": "preconnect",
			"href": "%PUBLIC_URL%/images/marco-d-cellamare-sm.webp",
			"as": "image"
		},
		{
			"rel": "preconnect",
			"href": "%PUBLIC_URL%/images/marco-d-cellamare.webp",
			"as": "image"
		}
	],
	"nav": {
		"path": "/",
		"children": [
			{
				"path": "/resume",
				"children": [
					{
						"path": "/front-end-developer",
						"hidden": true
					},
					{
						"path": "/back-end-developer",
						"hidden": true
					}
				]
			},
			{
				"path": "/projects"
			}
		]
	}
}